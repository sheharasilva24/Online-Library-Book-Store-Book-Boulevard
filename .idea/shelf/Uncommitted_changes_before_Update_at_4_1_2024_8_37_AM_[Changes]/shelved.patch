<+>const CheckoutDetail = require('../models/CheckoutDetail');\r\nconst User = require('../models/User'); // Assuming you have a User model\r\n\r\nexports.saveCheckoutDetails = async (req, res) => {\r\n    try {\r\n        const { email, transactionId } = req.body;\r\n\r\n        // Basic validation\r\n        if (!email || !transactionId) {\r\n            return res.status(400).json({ message: 'Missing required fields: email or transactionId.' });\r\n        }\r\n\r\n        // Find the user by email\r\n        const user = await User.findOne({ email: email });\r\n        if (!user) {\r\n            return res.status(404).json({ message: 'User not found.' });\r\n        }\r\n\r\n        // Calculate the subscription end date as 30 days from now\r\n        const subscriptionEndDate = new Date();\r\n        subscriptionEndDate.setDate(subscriptionEndDate.getDate() + 30);\r\n\r\n        // Create a new checkout detail instance with the user ID and the subscription end date, then save it\r\n        const newDetail = new CheckoutDetail({\r\n            email,\r\n            transactionId,\r\n            userId: user._id, // Use the found user's ID\r\n            subscriptionEndDate // Set the subscription end date\r\n        });\r\n        await newDetail.save();\r\n\r\n        res.status(201).json({ message: 'Checkout detail saved successfully', data: newDetail });\r\n    } catch (error) {\r\n        console.error('Error saving checkout detail:', error);\r\n        res.status(500).json({ message: 'Failed to save checkout detail' });\r\n    }\r\n};\r\n\r\nasync function updateUserSubscriptionStatus(userId) {\r\n    try {\r\n        // Find the latest checkout detail for the user\r\n        const latestCheckoutDetail = await CheckoutDetail.findOne({ userId: userId }).sort({ createdAt: -1 });\r\n        if (!latestCheckoutDetail) {\r\n            console.log('No checkout details found for user:', userId);\r\n            return;\r\n        }\r\n\r\n        // Determine the subscription status based on the latest checkout detail\r\n        const isSubscriptionActive = latestCheckoutDetail.subscriptionEndDate > new Date();\r\n\r\n        // Update the user's subscription status\r\n        await User.findByIdAndUpdate(userId, {\r\n            // Assuming your User model has a field named `subscriptionStatus`\r\n            subscriptionStatus: isSubscriptionActive ? 'active' : 'expired',\r\n        });\r\n\r\n        console.log(`User ${userId} subscription status updated.`);\r\n    } catch (error) {\r\n        console.error('Error updating user subscription status:', error);\r\n    }\r\n}\r\n
